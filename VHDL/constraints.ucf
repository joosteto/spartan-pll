NET "sstep" LOC = "D1" | IOSTANDARD = LVCMOS25 | DRIVE=16 | SLEW=FAST;	  #SIG_OUT_0
NET "state" LOC = "C1" | IOSTANDARD = LVCMOS25 | DRIVE=16 | SLEW=FAST;	  #SIG_OUT_1
NET "rst" LOC = "B1" | IOSTANDARD = LVCMOS25 | DRIVE=16 | SLEW=FAST;	  #SIG_OUT_2

NET "CLK0OUT" LOC = "A11"| IOSTANDARD = LVCMOS25 | DRIVE=16 | SLEW=FAST;	  #SIG_OUT_3
