#For XC6SKX16-FTG256 TVU
# Crystal Clock 25MHz onboard oscillator
NET "clkin" LOC = "J12" | IOSTANDARD = LVCMOS25 | PERIOD = 40.0ns ;

#NET "PI_SCLK" LOC = "T13"| IOSTANDARD = LVCMOS25; # W1_B16, P1-26
